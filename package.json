{
  "name": "exp-starter-api",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "coverage": "nyc mocha --recursive",
    "db:migrate": "node postgrator.js",
    "db:migrate:test": "NODE_ENV=test node postgrator.js",
    "db:seed": "for file in ./seeds/*.js; do node $file; done",
    "db:seed:windows": "for %f in (./seeds/*) do node ./seeds/%f",
    "lint": "eslint --ignore-path .gitignore .",
    "report": "nyc report --reporter=lcov",
    "start": "node ./bin/www",
    "test": "mocha --recursive && (yarn lint || npm lint)"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jsonwebtoken": "^8.0.1",
    "morgan": "~1.8.1",
    "pg": "^7.3.0",
    "postgrator": "^2.10.2"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "eslint": "^4.8.0",
    "expect": "^21.1.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "supertest": "^3.0.0"
  }
}
